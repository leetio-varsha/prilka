{
  "expo": {
    "name": "GamblSuite",
    "slug": "gamblsuite",
    "scheme": "gamblsuite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#EFEFEB",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a1d1e"
    },
    "assetBundlePatterns": [
      "assets/*",
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "associatedDomains": [
        "applinks:gamblsuite.onelink.me"
      ],
      "buildNumber": "1",
      "bundleIdentifier": "com.gambl.suite",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSUserTrackingUsageDescription": "We use your data to provide and improve the Service. By using the Service, you agree to the collection and use of information in accordance with this policy."
      }
    },
    "android": {
      "versionCode": 8,
      "package": "com.gambl.suite",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "gamblsuite.onelink.me",
              "pathPrefix": "/RGIK"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "gamblsuite"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "21e71949-ceba-482c-9bb1-0ddb6db011c6"
      }
    },
    "owner": "varshavsky.leetio",
    "plugins": [
      [
        "react-native-appsflyer",
        {}
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "We use your data to provide and improve the Service. By using the Service, you agree to the collection and use of information in accordance with this policy."
        }
      ]
    ],
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/21e71949-ceba-482c-9bb1-0ddb6db011c6"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
